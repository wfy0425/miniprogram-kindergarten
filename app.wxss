/**app.wxss**/

@import "weui/weui.wxss";
@import "vant/common/index.wxss";
:root {
    --logo-color: #82d9d0;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.logo-banner {
    width: 100%;
    padding: 35rpx 0 30rpx;
    display: flex;
    justify-content: center;
    border-top: 20rpx solid #82d9d0;
    background-color: #ffffff
}

.logo-banner image {
    width: 60%;
}