/* pages/students/index/index.wxss */

page {
    width: 100%;
    background-color: var(--background-color)
}

.box {
    display: flex;
    flex-direction: column;
    width: 690rpx;
    margin: auto;
    padding-top: 20rpx;
}

.text {
    position: relative;
    border-radius: 20rpx;
    background-color: #fff;
}

.text-box {
    display: flex;
    flex-direction: row;
}

.text-icon {
    padding-left: 10rpx;
    padding-right: 10rpx;
}

.text-occupy {
    padding: 10rpx;
}

.text-occupy1 {
    padding: 5rpx;
}

.text-hairline {
    padding: 3rpx;
    background-color: var(--background-color);
}

.text-text {
    color: var(--title-color);
    font-size: 30rpx;
}